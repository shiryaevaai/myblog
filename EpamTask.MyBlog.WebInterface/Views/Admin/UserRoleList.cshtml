@model IEnumerable<EpamTask.MyBlog.Entities.Role>

@if (Model.Count() == 0)
{
    <span>
        @Html.Label("У пользователя нет ролей")
    </span>
}
else
{
    <table>

    @foreach (var item in Model) {
        <tr>
            <td>
                <div>
                    @Html.DisplayFor(modelItem => item.RoleName)
                </div>
            </td>
        </tr>
    }
    </table>
}
